<h1>Listing profiles</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Description</th>
    <th>Address</th>
    <th>Profile url</th>
    <th>Blog url</th>
    <th>Facebook url</th>
    <th>Twitter url</th>
    <th>Linkedin</th>
    <th>Talktag</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @profiles.each do |profile| %>
  <tr>
    <td><%= profile.name %></td>
    <td><%= profile.email %></td>
    <td><%= profile.description %></td>
    <td><%= profile.address %></td>
    <td><%= profile.profile_url %></td>
    <td><%= profile.blog_url %></td>
    <td><%= profile.facebook_url %></td>
    <td><%= profile.twitter_url %></td>
    <td><%= profile.Linkedin %></td>
    <td><%= profile.talktag %></td>
    <td><%= link_to 'Show', profile %></td>
    <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
    <td><%= link_to 'Destroy', profile, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Profile', new_profile_path %>
